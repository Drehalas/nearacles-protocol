{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "globals": {
    "React": "readonly",
    "JSX": "readonly"
  },
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/no-unused-vars": "error",
    "react/no-unescaped-entities": "error",
    "react/react-in-jsx-scope": "off",
    "no-undef": "error"
  }
}
