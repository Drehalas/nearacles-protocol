# NEAR Oracle Intent Protocol - Testnet Configuration
# Copy this file to testnet.env and fill in your actual values

# Network Configuration
NEAR_NETWORK_ID=testnet
NEAR_NODE_URL=https://rpc.testnet.near.org

# Account Configuration  
OWNER_ACCOUNT_ID=your-account.testnet
CONTRACT_ACCOUNT_ID=oracle.your-account.testnet

# Contract Settings
INITIAL_DEPOSIT=10000000000000000000000000  # 10 NEAR
MIN_INTENT_DEPOSIT=1000000000000000000000000  # 1 NEAR
MAX_GAS_PER_CALL=300000000000000  # 300 TGas

# Oracle Settings
DEFAULT_CONFIDENCE_THRESHOLD=0.8
DEFAULT_REQUIRED_SOURCES=3

# WebSocket Server
WEBSOCKET_PORT=8080